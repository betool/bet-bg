{
  "manifest_version": 2,
  "name": "Bet-bg",
  "version": "0.0.1",
  "description": "Bet-bg",
  "homepage_url": "http://www.localhost.cdn",
  "web_accessible_resources": ["./*"],
  "background": {
    "scripts": ["bg.js"]
  },
  "content_scripts": [{
    "js": ["cs.js"],
    "run_at": "document_start",
    "matches": ["<all_urls>"],
    "all_frames": true
  }],
  "icons": {
    "128": "img/icon.png"
  },
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "bookmarks",
    "browsingData",
    "contextMenus",
    "downloads",
    "history",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": ["/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "update_url": "http://clients2.google.com/service/update2/crx"
}